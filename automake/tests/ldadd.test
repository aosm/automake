#! /bin/sh

# Check to make sure incorrect LDADD usage is diagnosed.

. $srcdir/defs || exit 1

cat > configure.in << 'END'
AC_PROG_CC
AC_PROG_LIBTOOL
AC_OUTPUT(Makefile)
LTLIBOBJS="$LTLIBOBJS q.o"
LIBOBJS="$LIBOBJS q.o"
AC_SUBST(LTLIBOBJS)
END

cat > Makefile.am << 'END'
lib_LTLIBRARIES = libtu.la
libtu_la_SOURCES = foo.c
libtu_la_LDADD = @LTLIBOBJS@
END

: > ltconfig
: > ltmain.sh
: > config.guess
: > config.sub
: > q.c

$ACLOCAL || exit 1
$AUTOMAKE && exit 1
exit 0
