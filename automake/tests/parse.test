#! /bin/sh

# Test to make sure backslash-newline is treated as whitespace.

. $srcdir/defs || exit 1

cat > configure.in << 'END'
AC_INIT
AM_INIT_AUTOMAKE(nonesuch, nonesuch)
AC_PROG_CC
AC_PROG_RANLIB
AC_OUTPUT(Makefile)
END

cat > Makefile.am << 'END'
bin_PROGRAMS = zot
zot_SOURCES = \
zar.c\
doz.c
END

$ACLOCAL || exit 1
$AUTOMAKE || exit 1

fgrep zar.cdoz Makefile.in && exit 1
exit 0
