#! /bin/sh

# Test to make sure LTLIBOBJS alone works ok.
# Report from Vadim Zeitlin.
required=libtool
. $srcdir/defs || exit 1

cat >> configure.in << 'END'
AC_SUBST(LTLIBOBJS)
AC_PROG_LIBTOOL
END

cat > Makefile.am << 'END'
lib_LTLIBRARIES = libtu.la
libtu_la_SOURCES = foo.c
libtu_la_LIBADD = @LTLIBOBJS@
END

: > ltconfig
: > ltmain.sh
: > config.guess
: > config.sub

$ACLOCAL || exit 1
$AUTOMAKE
